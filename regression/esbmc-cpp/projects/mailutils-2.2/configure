## SQL support








SQL_MODULES=


SQL_LIBOBJS=


SQL_LTLIBOBJS=


BUILD_SQL=




SQL_LOADABLE_MODULES=




add_sql_module() {
	case $SQL_MODULES in
	*$1*) { { $as_echo "$as_me:$LINENO: error: in \`$ac_pwd':" >&5
$as_echo "$as_me: error: in \`$ac_pwd':" >&2;}
{ { $as_echo "$as_me:$LINENO: error: $1 specified twice
See \`config.log' for more details." >&5
$as_echo "$as_me: error: $1 specified twice
See \`config.log' for more details." >&2;}
   { (exit 1); exit 1; }; }; };;
	*)    SQL_MODULES="$SQL_MODULES $1";;
	esac
}

