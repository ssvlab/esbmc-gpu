am_lf='
'
case `pwd` in
  *[\\\"\#\$\&\'\`$am_lf]*)
    { { $as_echo "$as_me:$LINENO: error: unsafe absolute working directory name" >&5
$as_echo "$as_me: error: unsafe absolute working directory name" >&2;}
   { (exit 1); exit 1; }; };;
esac
