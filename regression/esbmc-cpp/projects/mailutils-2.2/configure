  *[\\\"\#\$\&\'\`$am_lf\ \	]*)
    { { $as_echo "$as_me:$LINENO: error: unsafe srcdir value: \`$srcdir'" >&5
$as_echo "$as_me: error: unsafe srcdir value: \`$srcdir'" >&2;}
   { (exit 1); exit 1; }; };;
