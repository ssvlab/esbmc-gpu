int
main ()
{

  FILE *f = fopen ("conftest.val", "w");
  if (! f)
    return 1;
  if ((sizeof ($gltype) * CHAR_BIT) < 0)
    {
      long int i = longval ();
      if (i != (sizeof ($gltype) * CHAR_BIT))
	return 1;
      fprintf (f, "%ld", i);
    }
  else
    {
      unsigned long int i = ulongval ();
      if (i != (sizeof ($gltype) * CHAR_BIT))
	return 1;
      fprintf (f, "%lu", i);
    }
  /* Do not output a trailing newline, as this causes \r\n confusion
     on some platforms.  */
  return ferror (f) || fclose (f) != 0;

  ;
  return 0;
}
