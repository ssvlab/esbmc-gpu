with_flag_used=odbc

odbc_init() {
         case $with_flag_used in
	 yes|odbc|ODBC)
		check_lib_odbc
		;;
	 iodbc|iODBC|IODBC)
		check_lib_iodbc
		;;
	 no)	;;
	 esac
}


# Check whether --with-odbc was given.
if test "${with_odbc+set}" = set; then
  withval=$with_odbc; case $withval in
	 yes|odbc|ODBC|iodbc|iODBC|IODBC)
		add_sql_module "odbc"
		with_flag_used=$withval
		;;
	 no)    with_flag_used=$withval
		;;
	 *)	{ { $as_echo "$as_me:$LINENO: error: unsupported option value: $withval" >&5
$as_echo "$as_me: error: unsupported option value: $withval" >&2;}
   { (exit 1); exit 1; }; }
        esac
fi



### General SQL support

# Check whether --with-sql was given.
if test "${with_sql+set}" = set; then
  withval=$with_sql; for mod in `echo $withval | tr ':,' '  '`
         do
		if test -r $srcdir/sql/${mod}.c; then
			add_sql_module "${mod}"
		else
			{ { $as_echo "$as_me:$LINENO: error: No such SQL module: $mod" >&5
$as_echo "$as_me: error: No such SQL module: $mod" >&2;}
   { (exit 1); exit 1; }; }
		fi
	 done
fi


